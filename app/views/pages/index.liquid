	{% block javascripts %}
	<script>

		$(document).ready(function() {

			if ($( "#student-form" ).length) {
				var metrics = [
					[ "[name='student[last_name]']", 'presence', 'Last name is required' ],
                    [ "[name='student[first_name]']", 'presence', 'First name is required' ],
                    [ "[name='student[phone]']", 'presence', 'Phone Number is required' ],
                    [ "[name='student[email]']", 'presence', 'Email is required' ],
                    [ "[name='student[email]']", 'email', 'Invalid email address' ],
                    [ "[name='student[state]']", 'presence', 'State is required' ],
                    [ "[name='student[university]']", 'presence', 'University is required' ],
                    [ "[name='student[degree]']", 'presence', 'Degree Area is required' ],
                    [ "[name='student[stream]']", 'presence', 'Stream is required' ],
                    [ "[name='student[source]']", 'presence', 'Please tell us how you heard about CareerTrackers' ],
                    [ "[name='student[proof]']", 'presence', 'You must identify as Aboriginal or Torres Strait Islander to be eligible for the program' ],
				];

				$( "#student-form" ).nod( metrics , {
					groupSelector: ".form-group",
					groupClass: "has-error",
					helpSpanDisplay: "help-block small"
				});
			}

			if ($( "#employers-form" ).length) {
				var metrics = [
					[ "[name='employer[name]']", 'presence', 'Name is required' ],
					[ "[name='employer[company]']", 'presence', 'Company name is required' ],
					[ "[name='employer[email]']", 'presence', 'Email address is required' ],
					[ "[name='employer[email]']", 'email', 'Invalid email address' ],
					[ "[name='employer[phone]']", 'presence', 'Phone is required' ],
					[ "[name='employer[role]']", 'presence', 'Role is required' ],
					[ "[name='employer[city]']", 'presence', 'City is required' ]

				];

				$( "#employers-form" ).nod( metrics , {
					groupSelector: ".form-group",
					groupClass: "has-error",
					helpSpanDisplay: "help-block small"
				});
			}

			function postToGoogle(){
				var firstName = $("#firstName").val();
				var lastName = $("#lastName").val();
				var email = $("#email").val();
				var phone = $("#phone").val();
				var university = $("#university").val();
				var degree = $("#degree").val();
				var stream = $("#stream").val();
				var source = $("#source").val();
                var state = $("#state").val();
				var ajaxData = {"firstName" : firstName,
								"lastName" : lastName,
								"email" : email,
								"phone" : phone,
								"university" : university,
								"degree" : degree,
								"stream" : stream,
								"source" : source,
                                "state" : state
							   }

				$.ajax({
					type: "POST",
					//url: "https://docs.google.com/forms/d/e/1FAIpQLSejVdNguNuqfcD7QVaKTFbYff5AU6i-ZV_Av77UMQv9l-wRMg/formResponse?",
					url: "https://script.google.com/a/careertrackers.org.au/macros/s/AKfycbxmnCMs1yYh5FagsODmhh0I4nb_T0EdDWktAXAG7uQXwQM5hhrU/exec",
					data: JSON.stringify(ajaxData),
					dataType: "json",
					statusCode: {
						0: function() {
							console.log('success');//Success message
						},
						200: function() {
							console.log('success');//Success Message
						}
					},
					xhrFields: {
						withCredentials: true
					}
				});
			}

			$('#student-form').submit(function() {
				postToGoogle();
				//return false;
			});

		});
	</script>
