          <div class='form-group'>
            <label class='col-sm-1 control-label' for='signature'>
              Intern Signature
              <div class='small normal-font-weight'>
                (Type full name)
              </div>
            </label>
            <div class='col-sm-5'>
              <input class='form-control' id='signature' name='evaluation[signature]' type='text' value="{{params['signature']}}">
            </div>
          </div>
          <div class='form-group no-margin-bottom'>
            <label class='col-sm-1 control-label' for='date'>
              Date
            </label>
            <div class='col-sm-5'>
              <span class='form-control' id='date'>
                {{ today | localized_date: "%d/%m/%Y" }}
              </span>
            </div>
          </div>
        </fieldset>
      </div>
    </div>
	
    <div class='row'>
      <div class='col-sm-12'>
        <div class='form-group'>
          <div class='col-sm-2 pull-right'>
            <button class='btn orange-button' type='submit'>Submit</button>
          </div>
        </div>
      </div>
    </div>
