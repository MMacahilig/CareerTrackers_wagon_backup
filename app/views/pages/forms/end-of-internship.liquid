{% block javascripts %}

<script>

  $(document).ready(function() {
  
    $("[name='review[return_option]']").change(function(){
      var val = $(this).val();
      if (val) {
        if (val == "return_option_2") {
          $("#new-department").removeAttr("disabled");
        } else {
          $("#new-department").val("");
          $("#new-department").attr("disabled","disabled");
        }
      }
    });
