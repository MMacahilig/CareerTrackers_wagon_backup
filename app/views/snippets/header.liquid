<header>
  <div class='container-fluid white-background'>
    <div class='row'>
      <div class='col-md-7'>
        <div class='row'>
          <a href='/'>
            <img class='inline-display img-responsive' src="{{ 'careertrackers-logo.png' | theme_image_url }}">
          </a>
        </div>
      </div>
      <div class='col-md-5 screen-adapter' >
        <div class='row padded-top '>
          <div class='col-md-12 '>
            <ul class='list-unstyled contact'>
              <li class='contact_phone'>
                <i class='fa fa-phone'></i>
                Main Office:
                <a href='tel:+02 8090 5765'>02 8090 5765</a>
              </li>
              <li class='contact_email padded-top-small'>
                <i class='fa fa-envelope'></i>
                <a href='mailto:info@careertrackers.org.au'>info@careertrackers.org.au</a>
              </li>
            </ul>
          </div>
        </div>
        <div class='row padded-top hidden-xs'>
          <p class='lead padded-right col-md-12'>
            Creating opportunities that inspire excellence.
          </p>
        </div>
      </div>
    </div>
  </div>
  <nav class='navbar navbar-custom notrounded' role='navigation'>
    <div class='container-fluid'>
      <div class='navbar-header'>
        <button class='navbar-toggle' data-target='#bs-example-navbar-collapse-1' data-toggle='collapse' type='button'>
          <span class='sr-only'>Toggle navigation</span>
          <span class='icon-bar'></span>
          <span class='icon-bar'></span>
          <span class='icon-bar'></span>
        </button>
      </div>
      <div class='collapse navbar-collapse' id='bs-example-navbar-collapse-1'>
        <ul class='nav navbar-nav'>
          {% nav 'site', no_wrapper: true, active_class: "active", bootstrap: true, depth: 2 %}
        </ul>
        <ul class='nav navbar-nav navbar-right visible-lg'>
          <li>
            <a class='social-media-icons' href='http://www.linkedin.com/company/careertrackers-indigenous-internship-program' target='_blank'>
              <span class='fa-stack fa-lg'>
                <i class='fa fa-square-o fa-stack-2x'></i>
                <i class='fa fa-linkedin fa-stack-1x'></i>
              </span>
            </a>
          </li>
          <li>
            <a class='social-media-icons' href='https://facebook.com/CareerTrackers' target='_blank'>
              <span class='fa-stack fa-lg'>
                <i class='fa fa-square-o fa-stack-2x'></i>
                <i class='fa fa-facebook fa-stack-1x'></i>
              </span>
            </a>
          </li>
          <li>
            <a class='social-media-icons' href='https://twitter.com/CareerTrackers' target='_blank'>
              <span class='fa-stack fa-lg'>
                <i class='fa fa-square-o fa-stack-2x'></i>
                <i class='fa fa-twitter fa-stack-1x'></i>
              </span>
            </a>
          </li>
          <li>
            <a class='social-media-icons' href='https://www.youtube.com/channel/UCkjbo2NswXtIViu_CS6dsEA' target='_blank'>
              <span class='fa-stack fa-lg'>
                <i class='fa fa-square-o fa-stack-2x'></i>
                <i class='fa fa-youtube fa-stack-1x'></i>
              </span>
            </a>
          </li>
        </ul>
      </div>
    </div>
  </nav>
